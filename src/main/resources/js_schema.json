{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "The test schema",
  "description": "schema for test",
  "examples": [
    {
      "refreshToken": "ZmRzYWZkc2FmZHNhZg==",
      "vin": "LVSHFCAC012341234"
    }
  ],
  "required": [
    "refreshToken",
    "vin"
  ],
  "properties": {
    "refreshToken": {
      "$id": "#/properties/refreshToken",
      "type": "string"
    },
    "vin": {
      "$id": "#/properties/vin",
      "type": "string",
      "pattern" : "([\\s\\S]{1,50})",
      "minLength": 1,
      "maxLength": 50
    }
  }
}